# .env.example
# Copy this file to .env and fill in your values
# DO NOT commit .env to version control!

# Environment
DJANGO_ENV=development

# Security
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database
DB_ENGINE=sqlite
